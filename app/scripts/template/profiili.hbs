<div class="container">
    <div class="row-fluid">
        <div class="span8">
            <div class="media">
                <div class="pull-left">
                    <img src="/images/user.svg" alt="Käyttäjä" class="ugly-user">
                </div>
                <div class="media-body">
                    <h4 class="media-heading">lah2o</h4>
                    <p>Taso 1: 2p.</p>
                </div>
            </div>

            <div class="achievement">
                <img src="/images/1279581136.svg" alt="laheta_kuva" title="Lisää ensimmäinen haastekuva">
                <img src="/images/addafriend.svg" alt="lisaa_kaveri" title="Lisää ystävä">
                <img src="images/firstchallenge.svg" alt="suorita_ensimmainen_haaste" title="Suorita ensimmäinen haaste">
            </div>
            <div class="newsfeed">
            </div>
        </div>
        <div class="span4 well otsikot">
            <div>
                <h3>Omat haasteet</h3>
                <ul class="own-challenges thumbnails">
                    {{#each photos}}
                        <li class="span3">
                            <a href="/#/photos/{{id}}" class="thumbnail">
                                <img src="{{images.low_resolution.url}}" class="photo" width="100" height="100">
                            </a>
                        </li>
                    {{/each}}
                </ul>
            </div>
            <div>
                <h3>Kaverit</h3>
                <ul><li>Kamu</li>
                    <li>Frendi</li>
                    <li>Bestis</li>
                </ul>
            </div>
            <div>
                <h3>Tulevat tapahtumat</h3>
                <ul><li>Höntsäilypesistä Ahvenisjärven kentällä</li>
                </ul>
            </div>
            <div>
                <h3>Viimeaikaiset muutokset pisteissä</h3>
                <div class="progress progress-striped">
                    <div class="progress-marker left">Taso 1</div>
                    <div class="progress-marker right">Taso 2</div>
                    <div class="bar" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 80%;">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


